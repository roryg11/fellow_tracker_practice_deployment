<h1>Edit Goal</h1>

<%= form_for [@user, @goal], html: {class: "form-horizontal"} do |f| %>
  <%=f.label :description %>
  <%=f.text_field :description %>
  <%=f.label :due_date %>
  <%=f.date_field :due_date %>
  <%=f.label :completed %>
  <%=f.check_box :completed %>
  <%=f.submit "Update" %>
<% end %>
