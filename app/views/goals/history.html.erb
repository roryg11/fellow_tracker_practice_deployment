<div class="container">
  <h1><%= @user.first_name%> <%= @user.last_name %>'s Goal History</h1>
  <% @cohort_phase_array.each do |week| %>
  <table class="table">
    <thead>
      <h4>Week <%=week[:week_number]%>, <%=week[:start]%> - <%= week[:end] %></h4>
      <tr>
        <th>Description</th>
        <th>Due Date</th>
        <th>Completed</th>
        <th></th>
      </tr>
    </thead>
    <tbody>
      <% week[:goals].each do |goal| %>
        <tr>
          <td><%= goal.description %></td>
          <td><%= goal.due_date %></td>
          <td><%= goal.completed %></td>
          <td>CHECKBOX</td>
        </tr>
      <% end %>
    </tbody>
  </table>
  <% end %>
</div>
