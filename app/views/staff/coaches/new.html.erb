<h1>Add New Coach</h1>

<div class="container">
  <%= form_for [:staff, @coach] do |f| %>
    <%= f.label :first_name %>
    <%= f.text_field :first_name %>

    <%= f.label :last_name %>
    <%= f.text_field :last_name %>

    <%= f.label :email %>
    <%= f.email_field :email %>

    <%= f.submit "Create New Coach", id:"submit-user-button" %>
  <%end%>

</div>
