<h2>Fellows List</h2>

<table class="table table-bordered">
  <thead>
    <tr>
      <th>First Name</th>
      <th>Last Name</th>
      <th>Email</th>
      <th>Role</th>
    </tr>
  </thead>

  <tbody>
    <% @fellows.each do |fellow| %>
      <tr>
        <td><%= fellow.first_name %></td>
        <td><%= fellow.last_name %></td>
        <td><%= fellow.email %></td>
        <td><%= fellow.role %></td>
        <td><%= link_to "Show", staff_fellow_path(fellow) %></td>
        <td><%= link_to "Edit", edit_staff_fellow_path(fellow) %></td>
      </tr>
    <%end%>
  </tbody>
</table>
